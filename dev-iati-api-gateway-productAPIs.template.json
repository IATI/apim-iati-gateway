{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApimServiceName": {
      "type": "string"
    }
  },
  "resources": [
    {
      "name": "[concat(parameters('ApimServiceName'), '/starter/echo-api')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": []
    },
    {
      "name": "[concat(parameters('ApimServiceName'), '/unlimited/echo-api')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products/apis', parameters('ApimServiceName'), 'starter', 'echo-api')]"
      ]
    },
    {
      "name": "[concat(parameters('ApimServiceName'), '/no-authentication/iati-registry')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products/apis', parameters('ApimServiceName'), 'unlimited', 'echo-api')]"
      ]
    },
    {
      "name": "[concat(parameters('ApimServiceName'), '/starter/file-validator')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products/apis', parameters('ApimServiceName'), 'no-authentication', 'iati-registry')]"
      ]
    },
    {
      "name": "[concat(parameters('ApimServiceName'), '/unlimited/file-validator')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products/apis', parameters('ApimServiceName'), 'starter', 'file-validator')]"
      ]
    },
    {
      "name": "[concat(parameters('ApimServiceName'), '/private-routes/iati-validator-private')]",
      "type": "Microsoft.ApiManagement/service/products/apis",
      "apiVersion": "2019-01-01",
      "dependsOn": [
        "[resourceId('Microsoft.ApiManagement/service/products/apis', parameters('ApimServiceName'), 'unlimited', 'file-validator')]"
      ]
    }
  ]
}